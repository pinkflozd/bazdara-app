{
  "name": "bazdara-app",
  "version": "0.0.1",
  "description": "Vreme Piran, Podatki iz oceanografske boje Vide, Plimovanje morja",
  "author": "Luka Karinja",
  "license": "GPL-3.0-or-later",
  "repository": "pinkflozd/bazdara-app",
  "scripts": {
    "start": "polymer serve --hostname 0.0.0.0 --port 8000",
    "serve:static": "polymer serve --hostname 0.0.0.0 --port 8000 build/es5-bundled",
    "serve:prpl-server": "prpl-server --root server/build --port 8001",
    "bundle": "bundle-js ./leaflet-bundle.js -p | uglifyjs --compress --mangle -o leaflet.js",
    "build:static": "NODE_OPTIONS='--max-old-space-size=8192' polymer build",
    "build:prpl-server": "NODE_OPTIONS='--max-old-space-size=8192' polymer build --auto-base-path && gulp prpl-server",
    "build:firebase": "NODE_OPTIONS='--max-old-space-size=8192' polymer build --auto-base-path && gulp firebase",
    "deploy:firebase": "NODE_OPTIONS='--max-old-space-size=8192' polymer build --auto-base-path && gulp firebase && firebase deploy --only functions,hosting,database",
    "gulp:firebase": "gulp copy",
    "lint": "polymer lint",
    "test": "polymer test",
    "test:integration": "NODE_OPTIONS='--max-old-space-size=8192' polymer build # test that psk builds without error with the CLI",
    "postinstall": "cd server && npm install && cd ../functions && npm install",
    "generate-typings": "gen-typescript-declarations"
  },
  "dependencies": {
    "@fabricelements/skeleton-auth": "https://github.com/pinkflozd/skeleton-auth/tarball/master",
    "@polymer/app-layout": "^3.0.1",
    "@polymer/app-route": "^3.0.2",
    "@polymer/app-storage": "^3.0.2",
    "@polymer/iron-ajax": "^3.0.1",
    "@polymer/iron-collapse": "^3.0.1",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-form": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-iconset": "^3.0.1",
    "@polymer/iron-iconset-svg": "^3.0.1",
    "@polymer/iron-media-query": "^3.0.1",
    "@polymer/iron-meta": "^3.0.1",
    "@polymer/iron-pages": "^3.0.1",
    "@polymer/iron-scroll-threshold": "^3.0.1",
    "@polymer/iron-selector": "^3.0.1",
    "@polymer/neon-animation": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-dialog": "^3.0.1",
    "@polymer/paper-dropdown-menu": "^3.0.1",
    "@polymer/paper-fab": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.1",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-radio-button": "^3.0.1",
    "@polymer/paper-radio-group": "^3.0.1",
    "@polymer/paper-ripple": "^3.0.1",
    "@polymer/paper-spinner": "^3.0.1",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/paper-tabs": "^3.0.1",
    "@polymer/paper-toast": "^3.0.1",
    "@polymer/paper-toggle-button": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@types/hls.js": "^0.10.2",
    "@types/leaflet": "^1.2.14",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "canvas-gauges": "^2.1.5",
    "firebase": "^5.5.8",
    "firebase-admin": "~6.1.0",
    "highcharts": "^6.2.0",
    "hls.js": "^0.11.0",
    "iso8601-js-period": "^0.2.1",
    "leaflet": "^1.3.4",
    "leaflet-fullscreen": "^1.0.2",
    "leaflet-groupedlayercontrol": "^0.6.1",
    "leaflet-timedimension": "^1.1.0",
    "leaflet.locatecontrol": "^0.65.1",
    "leaflet.markercluster": "^1.4.1",
    "leaflet.measurecontrol": "^1.1.0",
    "polymer-redux": "^2.0.0-beta.2",
    "redux": "^4.0.1",
    "smoothscroll-polyfill": "^0.4.3",
    "web-animations-js": "^2.3.1"
  },
  "devDependencies": {
    "@polymer/gen-typescript-declarations": "^1.5.1",
    "axe-core": "^3.1.2",
    "babel-eslint": "^10.0.1",
    "bundle-js": "^1.0.2",
    "chai": "^4.2.0",
    "del": "^3.0.0",
    "eslint": "^5.9.0",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-html": "^4.0.6",
    "firebase-tools": "^6.0.1",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "jshint": "^2.9.6",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.0",
    "polyserve": "^0.27.13",
    "prpl-server": "^1.2.0",
    "puppeteer": "^1.10.0",
    "typescript": "^3.1.6",
    "typescript-lit-html-plugin": "^0.6.0",
    "uglify-js": "^3.4.9",
    "wct-browser-legacy": "^1.0.2"
  },
  "engines": {
    "node": ">=8.0"
  }
}
